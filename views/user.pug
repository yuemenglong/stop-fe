doctype html
html
    head
        title #{title}
        each meta in (metas||[])
            meta(name=meta.name content=meta.content description=meta.description http-equiv=meta.httpEquiv)
        script(src='//cdn.bootcss.com/jquery/2.2.3/jquery.js')
        script(src='//cdn.bootcss.com/react/15.6.1/react.js')
        script(src='//cdn.bootcss.com/react/15.6.1/react-dom.js')
        script(src='//cdn.bootcss.com/react-router/4.2.0/react-router.js')
        script(src='//cdn.bootcss.com/react-router-dom/4.2.2/react-router-dom.js')
        script(src='//cdn.bootcss.com/mobx/3.2.2/mobx.umd.js')
        script(src='//cdn.bootcss.com/mobx-react/4.2.2/index.js')
        script(src='//cdn.bootcss.com/lodash.js/4.12.0/lodash.js')
        script(src='//cdn.bootcss.com/moment.js/2.18.1/moment.js')
        script(src='//cdn.bootcss.com/bluebird/3.5.0/bluebird.js')
        link(href='//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css' rel='stylesheet')
        link(href=`/deploy/bundle/User/bundle.css?v=${v}` rel='stylesheet' type="text/css")

        link(href='/deploy/css/AdminLTE.css' rel='stylesheet' type="text/css")
        script(src='/deploy/js/adminlte.min.js')
        link(href='/deploy/css/dataTables.bootstrap.min.css' rel='stylesheet' type="text/css")

        script(src='/deploy/js/jquery.cookie.js')
        script(src="//cdn.bootcss.com/jquery-datetimepicker/2.5.14/jquery.datetimepicker.full.min.js")
        link(href="//cdn.bootcss.com/jquery-datetimepicker/2.5.14/jquery.datetimepicker.min.css" rel="stylesheet")
        script(src="/deploy/webuploader/webuploader.js")
        link(href="/deploy/webuploader/webuploader.css" rel="stylesheet")
        script(src="/deploy/chosen/chosen.jquery.js")
        link(href="/deploy/chosen/chosen.css" rel="stylesheet")
        script.
            $.ajaxSetup({contentType: "application/json; charset=utf-8"});
            window.FetchData = (function () {
                return !{init}
            })() || {}
            window.FetchData = _(window.FetchData).toPairs().map(function (p) {
                return [decodeURIComponent(p.name), p.value]
            }).fromPairs().value()
    body
        div(id="root") !{html}
        script(src=`/deploy/bundle/User/bundle.js?v=${v}`)
        script.
            var router = React.createElement(ReactRouterDOM.BrowserRouter, null,
                React.createElement(UserApp, null));
            ReactDOM.render(router, document.getElementById("root"));
